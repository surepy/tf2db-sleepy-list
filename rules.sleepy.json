{
	"$schema": "https://raw.githubusercontent.com/PazerOP/tf2_bot_detector/master/schemas/v3/rules.schema.json",
	"rules": [
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Vinesauce bots, sorry vinny! (u dont play tf2 anyway)",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"Vinesauce"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "marks anything with 'Fuck Nemo' (unreasonable name anyway)",
			"triggers": {
				"personaname_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"Fuck Nemo"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "marks anything with 'Dick Bigsby'",
			"triggers": {
				"personaname_text_match": {
					"case_sensitive": false,
					"mode": "equal",
					"patterns": [
						"Dick Bigsby"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "'fembot' by pay2lose; new bot in town? not too much instances",
			"triggers": {
				"personaname_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"youtube.com/@p2l"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"suspicious"
				]
			},
			"description": "a good 98% of people with this tag are cheating, verify and unmark otherwise.",
			"triggers": {
				"personaname_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"[VAC]"
					]
				}
			}
		},
		{
			"actions": {
				"transient_mark": [
					"cheater"
				]
			},
			"description": "bad characters",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"\u0e48"
					]
				}
			}
		},
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Innovate.tf new p2c it seems",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"INNOVATE.TF",
						"DISCORD.GG/8QRW2ZF9"
					]
				}
			}
		}
	]
}
